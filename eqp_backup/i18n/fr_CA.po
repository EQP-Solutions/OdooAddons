#. module: eqp_backup
#: model:mail.template,body_html:eqp_backup.email_template_data_automatic_backup_failed
msgid ""
"<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"background-color: white; border-collapse: collapse; margin-left: 20px;\">\n"
"                    <tr>\n"
"                        <td valign=\"top\" style=\"padding: 0px 10px;\">\n"
"                            <div>\n"
"                                <h2 style=\"text-align: center\">Attention!</h2>\n"
"                                <div style=\"color:grey;\">The Automatic\n"
"                                    Backup process\n"
"                                    <strong>\n"
"                                        <t t-out=\"object.name or ''\">BU Record 123</t>\n"
"                                    </strong>\n"
"                                    with the following parameters:\n"
"                                    <br>\n"
"                                    <ul>\n"
"                                        <li>\n"
"                                            <strong>ID:</strong>\n"
"                                            <t t-out=\"object.id  or ''\">0</t>\n"
"                                        </li>\n"
"                                        <li>\n"
"                                            <strong>Database:</strong>\n"
"                                            <t t-out=\"object.db_name or ''\">Database Name</t>\n"
"                                        </li>\n"
"                                        <li>\n"
"                                            <strong>Frequency:</strong>\n"
"                                            <t t-out=\"dict(object.fields_get(allfields=['frequency'])['frequency']['selection'])[object.frequency] or ''\">\n"
"                                                Weekly\n"
"                                            </t>\n"
"                                        </li>\n"
"                                        <li>\n"
"                                            <strong>Backup Type:</strong>\n"
"                                            <t t-out=\"object.server_id.backup_type or ''\">Local</t>\n"
"                                        </li>\n"
"                                    </ul>\n"
"                                    has encountered a failure.\n"
"                                    <br>\n"
"                                    <br>\n"
"                                </div>\n"
"                            </div>\n"
"                            <div style=\"font-size: 13px; margin: 0px; padding: 0px;\">\n"
"                                This is an automatic notification to inform you about the failure in the Automatic\n"
"                                Backup process.\n"
"                                <br>\n"
"                                <br>\n"
"                                <t t-if=\"object.user_id\">\n"
"                                    <strong t-out=\"object.user_id.name or ''\">Mitchell Admin</strong>, you are receiving\n"
"                                    this notification because you registered as the responsible to this\n"
"                                    <strong>\n"
"                                        <t t-out=\"object.name or ''\">BU Record 123</t>\n"
"                                    </strong>\n"
"                                    Backup Record.\n"
"                                    <br>\n"
"                                    <br>\n"
"                                </t>\n"
"                                <hr width=\"97%\" style=\"background-color: rgb(204,204,204); border: medium none; clear: both; display: block; font-size: 0px; min-height: 1px; line-height: 0; margin: 16px 0px 16px 0px;\">\n"
"                                <h3 style=\"color:#9A6C8E;\">\n"
"                                    <strong>Recommended Actions:</strong>\n"
"                                </h3>\n"
"                                <p>\n"
"                                    <strong>Error Details:</strong>\n"
"                                    <br>\n"
"                                    Please check the system log for more information about the encountered error.\n"
"                                </p>\n"
"                                <p>\n"
"                                    <strong>Confirmation Status:</strong>\n"
"                                    <br>\n"
"                                    Ensure that both the Backup Record and the associated server are in a confirmed\n"
"                                    status.\n"
"                                </p>\n"
"                                <p>\n"
"                                    <strong>Testing Tools:</strong>\n"
"                                    <br>\n"
"                                    If the documents are in a confirmed status, use the testing tools on each of the\n"
"                                    documents.\n"
"                                </p>\n"
"                            </div>\n"
"                        </td>\n"
"                    </tr>\n"
"                </table>\n"
"            "
msgstr ""
"<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"background-color: white; border-collapse: collapse; margin-left: 20px;\">\n"
"                    <tr>\n"
"                        <td valign=\"top\" style=\"padding: 0px 10px;\">\n"
"                            <div>\n"
"                                <h2 style=\"text-align: center\">Attention !</h2>\n"
"                                <div style=\"color:grey;\">Le processus de sauvegarde automatique\n"
"                                    <strong>\n"
"                                        <t t-out=\"object.name or ''\">Enregistrement BU 123</t>\n"
"                                    </strong>\n"
"                                    avec les paramètres suivants :\n"
"                                    <br>\n"
"                                    <ul>\n"
"                                        <li>\n"
"                                            <strong>ID :</strong>\n"
"                                            <t t-out=\"object.id  or ''\">0</t>\n"
"                                        </li>\n"
"                                        <li>\n"
"                                            <strong>Base de données :</strong>\n"
"                                            <t t-out=\"object.db_name or ''\">Nom de la base de données</t>\n"
"                                        </li>\n"
"                                        <li>\n"
"                                            <strong>Fréquence :</strong>\n"
"                                            <t t-out=\"dict(object.fields_get(allfields=['frequency'])['frequency']['selection'])[object.frequency] or ''\">\n"
"                                                Hebdomadaire\n"
"                                            </t>\n"
"                                        </li>\n"
"                                        <li>\n"
"                                            <strong>Type de sauvegarde :</strong>\n"
"                                            <t t-out=\"object.server_id.backup_type or ''\">Local</t>\n"
"                                        </li>\n"
"                                    </ul>\n"
"                                    a rencontré une défaillance.\n"
"                                    <br>\n"
"                                    <br>\n"
"                                </div>\n"
"                            </div>\n"
"                            <div style=\"font-size: 13px; margin: 0px; padding: 0px;\">\n"
"                                Il s'agit d'une notification automatique pour vous informer du résultat du processus de sauvegarde automatique.\n"
"                                <br>\n"
"                                <br>\n"
"                                <t t-if=\"object.user_id\">\n"
"                                    <strong t-out=\"object.user_id.name or ''\">Mitchell Admin</strong>, vous recevez\n"
"                                    cette notification car vous êtes enregistré en tant que responsable de cet\n"
"                                    <strong>\n"
"                                        <t t-out=\"object.name or ''\">Enregistrement BU 123</t>\n"
"                                    </strong>\n"
"                                    de sauvegarde.\n"
"                                    <br>\n"
"                                    <br>\n"
"                                </t>\n"
"                                <hr width=\"97%\" style=\"background-color: rgb(204,204,204); border: medium none; clear: both; display: block; font-size: 0px; min-height: 1px; line-height: 0; margin: 16px 0px 16px 0px;\">\n"
"                                <h3 style=\"color:#9A6C8E;\">\n"
"                                    <strong>Actions recommandées :</strong>\n"
"                                </h3>\n"
"                                <p>\n"
"                                    <strong>Détails de l'erreur :</strong>\n"
"                                    <br>\n"
"                                    Veuillez consulter le journal système pour plus d'informations sur l'erreur rencontrée.\n"
"                                </p>\n"
"                                <p>\n"
"                                    <strong>État de confirmation :</strong>\n"
"                                    <br>\n"
"                                    Assurez-vous que l'enregistrement de sauvegarde et le serveur associé sont dans un état confirmé.\n"
"                                </p>\n"
"                                <p>\n"
"                                    <strong>Outils de test :</strong>\n"
"                                    <br>\n"
"                                    Si les documents sont dans un état confirmé, utilisez les outils de test sur chacun des\n"
"                                    documents.\n"
"                                </p>\n"
"                            </div>\n"
"                        </td>\n"
"                    </tr>\n"
"                </table>\n"
"            "

